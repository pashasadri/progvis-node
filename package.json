{
  "name": "progvis",
  "version": "0.1.2",
  "description": "Client library for ProgVis. Use it to instrument your cron and batch jobs.  Track progress on progvis.com",
  "keywords": [
    "progress",
    "progressbar",
    "monitoring",
    "cron",
    "batch"
  ],
  "files": ["dist/progvis.js", "src/progvis.js"],
  "main": "dist/progvis.js",
  "module": "src/progvis.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf dist && babel src/*.js -d dist"
  },
  "author": "Pasha Sadri",
  "license": "MIT",
  "repository": "github:pashasadri/progvis-node",
  "dependencies": {
    "lodash": "^4.17.15",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2"
  }
}
